{
  "name": "shifa-care-ai-insights",
  "version": "1.0.0",
  "description": "Shifa Care AI Insights - نظام شفاء كير للذكاء الاصطناعي",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "deploy": "git add . && git commit -m \"🔄 تحديث تلقائي\" && git push origin main",
    "deploy:auto": "powershell -ExecutionPolicy Bypass -File auto-deploy.ps1",
    "deploy:bash": "bash auto-deploy.sh",
    "deploy:check": "git status",
    "deploy:force": "git add . && git commit -m \"🔄 تحديث إجباري\" && git push origin main --force"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@supabase/supabase-js": "^2.21.0",
    "class-variance-authority": "^0.6.0",
    "clsx": "^1.2.1",
    "i18next": "^21.8.16",
    "i18next-browser-languagedetector": "^6.1.4",
    "lucide-react": "^0.263.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.16.9",
    "react-router-dom": "^6.3.0",
    "recharts": "^3.0.2",
    "tailwind-merge": "^1.13.2",
    "tailwindcss-animate": "^1.0.6"
  },
  "devDependencies": {
    "@types/react": "^17.0.65",
    "@types/react-dom": "^17.0.20",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.9.5",
    "vite": "^4.4.5"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
